<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/admity_bacground_img"
    tools:context=".views.FeesActivity">
    <include layout="@layout/layout_toolbar"
        android:id="@+id/Feeslayout_toolbaar"
        android:layout_height="110dp"
        android:layout_width="match_parent" />
    <com.google.android.material.tabs.TabLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:id="@+id/fees_tabLayout" />
    <androidx.viewpager.widget.ViewPager
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fees_viewPager"
        android:layout_below="@id/fees_tabLayout"/>
    <include layout="@layout/content_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>
